class Person {
	method toString {
		return: firstName + " " + lastName
	}

	constructor |first, last| {
		this::firstName = first
		this::lastName = last
	}
}

person := Person("John", "Kowalski")
maggie := Person("Maggie", "Smith")

print("If things went smoothly, this should say John Kowalski:", person)
print("And this, Maggie Smith:", maggie)
