i := 2;

while (i < 20) {
	print(i);

	if (i % 2 == 0) {
		print("Divisible by 2.");
	} elseif (i % 3 == 0) {
		print("Divisible by 3, but not by 2.");
	} elseif (i % 5 == 0) {
		print("Divisible by 5, but not by 2 or 3.");
	} else {
		print("No prime factors less than 7.");
	};

	i = i + 1;
};
