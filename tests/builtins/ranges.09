print("This should print numbers from 1 to 5:")
for (i) in (1..5) {
	print(i)
}

print("This should print numbers from 1 to 4:")
for (i) in (1...5) {
	print(i)
}

print("Indexing should work, so this should print 1 4:", (1..4)[0], (1..4)[-1])

slice := (1..5)[1...(-1)]
print("Slicing should work, so this should print 2, 3, 4:", ", ".join(slice))
